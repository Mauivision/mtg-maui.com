{
  "name": "mtg-maui-league",
  "version": "1.0.0",
  "description": "MTG Maui League - Magic: The Gathering Tournament Platform",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3003",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "postinstall": "prisma generate",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "prisma:seed": "prisma db seed",
    "prisma:reset": "prisma migrate reset",
    "fix:prisma": "node scripts/fix-prisma-imports.js",
    "logger:migrate": "node scripts/replace-console-with-logger.js",
    "clean": "rm -rf .next out dist",
    "clean:all": "rm -rf .next out dist node_modules/.cache",
    "verify": "npm run type-check && npm run lint && npm run build",
    "deploy": "npx vercel --prod"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.9.1",
    "@babel/runtime": "^7.25.9",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^5.22.0",
    "@tanstack/react-query": "^5.79.0",
    "bcryptjs": "^2.4.3",
    "chart.js": "^4.4.9",
    "date-fns": "^3.6.0",
    "framer-motion": "^10.18.0",
    "jose": "^5.9.0",
    "jspdf": "^4.0.0",
    "next": "15.3.8",
    "next-auth": "^4.24.11",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.5.2",
    "react-icons": "^5.5.0",
    "resend": "^6.7.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20.10.0",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.54.0",
    "eslint-config-next": "^15.5.4",
    "postcss": "^8.5.4",
    "prisma": "^5.22.0",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
